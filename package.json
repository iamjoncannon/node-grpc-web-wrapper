{
  "name": "node-grpc-web-wrapper",
  "version": "1.0.9",
  "description": "grpc-web node in process translator",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "npx tsc --build --clean",
    "test": "npm run clean && jest",
    "test:cov": "npx tsc --build --clean && jest --coverage",
    "build": "npm run clean && npx tsc",
    "start": "npm run build && node ./dist/testService.js",
    "test:unary": "npm run build && TEST=unary node ./dist/mock/clients/client.js",
    "test:stream": "npm run build && TEST=stream node ./dist/mock/clients/client.js"
  },
  "keywords": [
    "grpc-web",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "async": "^3.2.4",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "typescript": "^5.0.4",
    "xhr2": "^0.2.1"
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/async": "^3.2.20",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.ts"
    ],
    "setupFiles": [
      "./test-setup.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "lines": 100,
        "branches": 100
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs",
      "jsx",
      "ts",
      "d.ts",
      "tsx",
      "json",
      "node"
    ]
  }
}
